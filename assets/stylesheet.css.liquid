.icn {
  position: absolute;
  text-align: center;
  padding: 0 10px;
  font-family: {{ main_font.family }};
  font-size: 12px;

  {% if settings.icon_position == 'top_left' %}
  top: 0; left: 0;
  {% elsif settings.icon_position == 'top_right' %}
  top: 0; right: 0;
  {% elsif settings.icon_position == 'bottom_left' %}
  bottom: 0; left: 0;
  {% elsif settings.icon_position == 'bottom_right' %}
  bottom: 0; right: 0;
  {% endif %}
  {% if settings.icon_shape == 'circle' %}
    width: 50px;
    height: 50px;
    border-radius: 50%;
    line-height: 50px;
  {% elsif settings.icon_shape == 'rectangle' %}
    padding: 8px 10px;
  {% endif %}
}

{% if settings.new_icon %}
  .new {
    z-index: 3;
    background: {{ settings.new_background }};
    color: {{ settings.new_color }};
  }
  {% else %}
  .new {
    display: none;
  }
  {% endif %}

{% if settings.sale_icon %}
  .sale-item {
    z-index: 4;
    background: {{ settings.sale_icon_background }};
    color: {{ settings.sale_icon_color }};
  }
  {% else %}
  .sale-item {
    display: none;
  }
  {% endif %}

  {% if settings.sold_out_icon %}
  .so {
    z-index: 5;
    background: {{ settings.so_background }};
    color: {{ settings.so_color }};
    {% if settings.icon_shape == 'circle' %}
      line-height: 1.4em;
      padding-top: 9px;
    {% endif %}
  }
  {% else %}
  .so {
    display: none;
  }
  {% endif %}

